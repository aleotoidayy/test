deleteHeader = type=http-request, pattern=^https:\/\/api\.revenuecat\.com\/.+\/(receipts|subscribers), script-path=https://raw.githubusercontent.com/aleotoidayy/klinhh/klinhh/deleteheader.js, timeout=60

